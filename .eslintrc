{ 
  "env": { 
    "browser": true, 
    "jest": true,
    "node": true
  }, 
  "extends": [ 
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ], 
  "parser": "@typescript-eslint/parser", 
  "parserOptions": { 
    "ecmaVersion": 12, 
    "sourceType": "module" 
  }, 
  "plugins": [ 
    "@typescript-eslint", 
    "prettier", 
    "import" 
  ], 
  "rules": { 
    "prefer-arrow-callback": "off", 
    "no-var": "error", 
    "no-dupe-keys": "error", 
    "prettier/prettier": "error" 
  }
}